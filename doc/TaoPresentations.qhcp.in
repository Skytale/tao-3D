<?xml version=\"1.0\" encoding=\"UTF-8\"?>
<QHelpCollectionProject version=\"1.0\">
    <assistant>
	<!-- Do not set title here because you cannot specify translations -->
	<!-- We keep default title wich is correctly translated by tr() in ../assistant -->
	<!-- <title>Tao Presentations Help</title> -->
        <cacheDirectory>Taodyne/$$APP_NAME</cacheDirectory>
        <startPage>qthelp://com.taodyne.taopresentations.$$TAO_VERSION/TaoPresentations/index.html</startPage>
        <aboutDialog>
            <file>about_help.html</file>
            <file language=\"fr\">about_help_fr.html</file>
        </aboutDialog>
        <enableDocumentationManager>false</enableDocumentationManager>
        <enableAddressBar>false</enableAddressBar>
        <enableFilterFunctionality>true</enableFilterFunctionality>
    </assistant>
</QHelpCollectionProject>

