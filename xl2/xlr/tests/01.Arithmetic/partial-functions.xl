badabaf x,y,z -> debug "Three arguments: ", x, y, z

bidibif a -> badabaf a, 3

bidibif 2,3
badabaf 1,2,3

machin x:real, y:real, z:real, a:real, b:real -> debug "Machin 5", a, b, x, y, z
machin x:real, a:real, b:real -> debug "Machin 3", a, b, x

müsli {x:integer/y:integer},z -> machin z, x, y


data x,y

bidibif 2,3
badabaf 1,2,3
müsli {1/3},2
müsli {777/222},(6,4,2)
