PREFIX(Caption, tree, "status", PARM(t, text), RTAO(status(self, t)))

PREFIX(RotateX, tree, "rotatex", PARM(rx, real), RTAO(rotate(self, rx,1,0,0)))
PREFIX(RotateY, tree, "rotatey", PARM(ry, real), RTAO(rotate(self, ry,0,1,0)))
PREFIX(RotateZ, tree, "rotatez", PARM(rz, real), RTAO(rotate(self, rz,0,0,1)))
PREFIX(Rotate, tree, "rotate",
       PARM(ra, real) PARM(rx, real) PARM(ry, real) PARM(rz, real),
       RTAO(rotate(self, ra, rx, ry, rz)))

PREFIX(TransX, tree, "translatex", PARM(x, real), RTAO(translate(self, x,0,0)))
PREFIX(TransY, tree, "translatey", PARM(y, real), RTAO(translate(self, 0,y,0)))
PREFIX(TransZ, tree, "translatez", PARM(z, real), RTAO(translate(self, 0,0,z)))
PREFIX(Translate, tree, "translate",
       PARM(x, real) PARM(y, real) PARM(z, real),
       RTAO(translate(self, x, y, z)))

PREFIX(ScaleX, tree, "scalex", PARM(x, real), RTAO(scale(self, x,1,1)))
PREFIX(ScaleY, tree, "scaley", PARM(y, real), RTAO(scale(self, 1,y,1)))
PREFIX(ScaleZ, tree, "scalez", PARM(z, real), RTAO(scale(self, 1,1,z)))
PREFIX(Scale, tree, "scale",
       PARM(x, real) PARM(y, real) PARM(z, real),
       RTAO(scale(self, x, y, z)))

PREFIX(Locally, tree, "locally", PARM(t, tree), RTAO(locally(self, &t)))
PREFIX(PageSize, tree, "page_size", PARM(w, integer) PARM(h, integer),
       RTAO(pagesize(self, w,h)))
PREFIX(Page, tree, "page", PARM(p, tree),
       RTAO(page(self, &p)))
PREFIX(PageWidth, integer, "page_width", , RTAO(page_width(self)))
PREFIX(PageHeight, integer, "page_height", , RTAO(page_height(self)))

PREFIX(Refresh, tree, "refresh", PARM(delay, real), RTAO(refresh(self, delay)))

PREFIX(DaySeconds, tree, "time", , RTAO(time(self)))
PREFIX(PageSeconds, tree, "page_time", , RTAO(page_time(self)))
PREFIX(Selectable, boolean, "selectable",, RTAO(selectable(self, true)))
PREFIX(NotSelectable, boolean, "not_selectable",, RTAO(selectable(self, false)))

PREFIX(ColorRGBA, tree, "color",
       PARM(r, real) PARM(g, real) PARM(b, real) PARM(a, real),
       RTAO(color(self, r,g,b, a)))
PREFIX(ColorRGB, tree, "color", PARM(r, real) PARM(g, real) PARM(b, real),
        RTAO(color(self, r,g,b, 1.0)))

PREFIX(Filled, tree, "filled", , RTAO(filled(self)))
PREFIX(Hollow, tree, "hollow", , RTAO(hollow(self)))
PREFIX(LineWidth, tree, "line_width", PARM(lw, real), RTAO(linewidth(self, lw)))

PREFIX(Polygon, tree, "polygon", PARM(t, tree), RTAO(polygon(self, &t)))

PREFIX(Vertex3, tree, "vertex",
       PARM(x, real) PARM(y, real) PARM(z, real), RTAO(vertex(self, x, y, z)))
PREFIX(Vertex2, tree, "vertex",
       PARM(x, real) PARM(y, real), RTAO(vertex(self, x, y, 0.0)))

PREFIX(Sphere, tree, "sphere",
       PARM(x, real) PARM(y, real) PARM(z, real)
       PARM(r, real) PARM(slices, integer) PARM(stacks, integer),
       RTAO(sphere(self, x, y, z, r, slices, stacks)))
PREFIX(Svg, tree,"svg", PARM(img, text), RTAO(svg(self, img)))
PREFIX(Texture, tree, "texture", PARM(n, text), RTAO(texture(self, n)))
PREFIX(TexCoord, tree, "texcoord", PARM(x, real) PARM(y, real),
       RTAO(texCoord(self, x, y)))
PREFIX(Circle, tree, "circle", PARM(x, real) PARM(y, real) PARM(r, real),
       RTAO(circle(self, x, y, r)))
PREFIX(CircularSector, tree, "circular_sector",
       PARM(x, real) PARM(y, real) PARM(r, real) PARM(a, real) PARM(b, real),
       RTAO(circularSector(self, x, y, r, a, b)))
PREFIX(RoundedRectangle, tree, "rounded_rectangle",
       PARM(x, real) PARM(y, real) PARM(w, real) PARM(h, real) PARM(r, real),
       RTAO(roundedRectangle(self, x, y, w, h, r)))
PREFIX(Rectangle, tree, "rectangle",
       PARM(x, real) PARM(y, real) PARM(w, real) PARM(h, real),
       RTAO(rectangle(self, x, y, w, h)))
PREFIX(RegularStarPolygon, tree, "regular_star_polygon",
       PARM(x, real) PARM(y, real) PARM(r, real)
       PARM(p, integer) PARM(q, integer), 
       RTAO(regularStarPolygon(self, x, y, r, p, q)))

POSTFIX(FromCm,   real, PARM(x, real), "cm", RTAO(fromCm(self, x)))
