PREFIX(FontStrikeout, tree, "strikeout",, RTAO(fontStrikeout(self)))
PREFIX(FontNoStrikeout, tree, "no_strikeout",, RTAO(fontStrikeout(self, false)))
PREFIX(FontStretch, tree, "stretch", PARM(s,integer),RTAO(fontStretch(self,s)))

PREFIX(TextLeft, tree, "align_left",, RTAO(align(self,Qt::AlignLeft)))
PREFIX(TextRight, tree, "align_right",, RTAO(align(self,Qt::AlignRight)))
PREFIX(TextCenter, tree, "align_center",, RTAO(align(self,Qt::AlignHCenter)))
PREFIX(TextJustify, tree, "align_justify",, RTAO(align(self,Qt::AlignJustify)))
PREFIX(TextTop, tree, "align_top",, RTAO(align(self,Qt::AlignTop)))
PREFIX(TextBottom, tree, "align_bottom",, RTAO(align(self,Qt::AlignBottom)))
PREFIX(TextMiddle, tree, "align_middle",, RTAO(align(self,Qt::AlignVCenter)))

PREFIX(TextSpan, tree,"text", PARM(t, text), RTAO(textSpan(self, t)))

PREFIX(Flow, tree, "flow", , RTAO(flow(self)))
PREFIX(Frame, tree, "frame",
